import re, requests, xbmc, xbmcaddon, xbmcgui, time
def GetLSProData(page_data,Cookie_Jar,m, txt1='Inserte la url del Addon'):
 myheaders = {'User-Agent': 'vlc', 'Referer': ''}
 path = xbmc.translatePath("special://profile/addon_data/" + "plugin.video.cristalazul/$doregex[addons]")
 addon = xbmcaddon.Addon('plugin.video.cristalazul')
 profile = xbmc.translatePath(addon.getAddonInfo('profile').decode('utf-8'))
 
 try:
  f = open(path, 'r')
  key = f.read()
  f.close()
  
 except:
  solver = InputWindow( txt=txt1)
  key = solver.get().replace('&amp;','&')
  f = open(path, 'w')
  f.write(key)
  f.close()

 full = key
 return full

  
class InputWindow(xbmcgui.WindowDialog):
 def __init__(self, *args, **kwargs):
  self.txt = kwargs.get('txt','')
  xbmcgui.WindowXMLDialog.__init__(self)
  #self.addControl(self.txt)
  path = xbmcaddon.Addon().getAddonInfo('path')
  self.kbd = xbmc.Keyboard()
  self.kbd.setHeading( self.txt)
 def get(self):
  self.show()
  time.sleep(2)
  self.kbd.doModal()
  if (self.kbd.isConfirmed()):
      text = self.kbd.getText()
      self.close()
      return text
  self.close()
  return False  
