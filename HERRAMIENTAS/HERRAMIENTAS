import urllib, re, requests, xbmcgui, xbmc, xbmcaddon, time
import os
def GetLSProData(page_data,Cookie_Jar,m):

  dialog = xbmcgui.Dialog()
  ret = dialog.select('Borra los addons ', ['[COLOR red]No borrar niguno[/COLOR]', '[COLOR white]borrar addon 1[/COLOR]', '[COLOR white]borrar addon 2[/COLOR]', '[COLOR white]borrar addon 3[/COLOR]', '[COLOR white]borrar addon 4[/COLOR]', '[COLOR white]borrar addon 5[/COLOR]', '[COLOR white]borrar addon 6[/COLOR]', '[COLOR white]borrar addon 7[/COLOR]'])
  MEMOrefs = ['nigun_addon', 'Addon_1', 'Addon_2', 'Addon_3', 'Addon_4', 'Addon_5', 'Addon_6', 'Addon_7']
  selected = MEMOrefs[ret]

  try:
    path = xbmc.translatePath("special://profile/addon_data/" + "plugin.video.cristalazul/")
    addon = xbmcaddon.Addon('plugin.video.cristalazul')
    addon_version = addon.getAddonInfo('version')
    profile = xbmc.translatePath(addon.getAddonInfo('profile').decode('utf-8'))
      
    os.remove (path + selected)
    
    xbmc.executebuiltin("XBMC.Notification(Hecho !, Addon Borrado\nCon exito !, 2000)")
    return 'https://pastebin.com/raw/ibz5CkWb'    ### YOUR URL HERE ### (or /path/local/file.xml)

  except:
    pass
 
class InputWindow(xbmcgui.WindowDialog):
    def __init__(self, *args, **kwargs):
        self.txt = kwargs.get('txt','')
        xbmcgui.WindowXMLDialog.__init__(self)
        #self.addControl(self.txt)
        path = xbmcaddon.Addon().getAddonInfo('path')
        self.kbd = xbmc.Keyboard()
        self.kbd.setHeading( self.txt)

    def get(self):
        self.show()
        time.sleep(2)
        self.kbd.doModal()
        if (self.kbd.isConfirmed()):
            text = self.kbd.getText()
            self.close()
            return text
        self.close()
        return False
 
